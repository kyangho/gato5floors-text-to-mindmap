{
  "name": "text-to-mindmap",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prisma": {
    "schema": "./src/server/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite --open",
    "dev:server": "nodemon --watch ./src/server --watch ./.env.development ./src/server/index.cjs",
    "start:client": "vite",
    "start:server": "nodemon --watch ./src/server --watch ./.env.development ./src/server/index.cjs",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "vite build --mode production",
    "knex": "knex init --knexfile ./src/server/database/knexfile.cjs",
    "preview": "vite preview",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "rimraf ./src/config/jest/coverage && npm test -- --coverage",
    "migrate:make": "knex migrate:make --knexfile ./src/server/database/knexfile.cjs",
    "migrate:latest": "knex migrate:latest --knexfile ./src/server/database/knexfile.cjs",
    "migrate:down": "knex migrate:down --knexfile ./src/server/database/knexfile.cjs",
    "postinstall": "npm run migrate:latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.8",
    "@mui/material": "^5.14.8",
    "@prisma/client": "4.7.1",
    "@react-oauth/google": "^0.11.1",
    "axios": "1.3.3",
    "bookshelf": "1.2.0",
    "concurrently": "8.2.1",
    "cors": "2.8.5",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "dotenv-cli": "^7.3.0",
    "express": "4.18.2",
    "joi": "17.8.4",
    "jsonwebtoken": "^9.0.2",
    "knex": "0.21.21",
    "lodash": "4.17.21",
    "mermaid": "^10.4.0",
    "prisma": "4.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-icons": "4.10.1",
    "react-router-dom": "6.4.3",
    "reactflow": "11.8.3",
    "sqlite3": "5.1.6",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@reduxjs/toolkit": "1.9.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "^10.4.15",
    "babel-jest": "29.3.1",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "cross-env": "7.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "less": "4.1.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.29",
    "postcss-less": "6.0.0",
    "prettier": "2.7.1",
    "react-apexcharts": "1.4.1",
    "react-fast-compare": "3.2.2",
    "react-intl": "6.4.2",
    "react-quill": "2.0.0",
    "react-redux": "8.0.5",
    "react-scripts": "5.0.1",
    "react-slick": "0.29.0",
    "rimraf": "3.0.2",
    "stylelint": "14.15.0",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "stylelint-prettier": "2.0.0",
    "tailwindcss": "^3.3.3",
    "vite": "3.2.4",
    "vite-plugin-html-env": "1.2.7"
  }
}
